{
	"manifest_version": 2,
	"name": "Agregore Renderer",
	"version": "1.0.0",

	"description": "Render Markdown/Gemini/JSON",

	"permissions": [
		"tabs",
		"webNavigation",
		"activeTab",
		"<all_urls>"
	],

	"background": {
		"scripts": ["background.js"]
	},

	"web_accessible_resources": [
		"bundle-gemini.js",
		"bundle-markdown.js",
		"bundle-json.js"
	]
}
